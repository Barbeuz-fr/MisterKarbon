<div class="container">
  <div class="row">
    <div class="col-6">
      <div class="tab-button text-center">
        <%= link_to "Progress", report_path(@report)%>
      </div>
    </div>
    <div class="col-6">
      <div class="tab-button active-tab">
        <h2 >Result</h2>
      </div>
    <div >
  </div>
</div>


  <div class="container mt-3 mb-2">
    <h3>Your report : <%= @report.name %> - <%= @report.year %></h3>
  </div>

<div class="container">
  <ul>
    <% @output_array.each do |item| %>
    <li>
      <p><%= item[:emission_module_name] %> - <%= item[:orga_scope_name] %> - <%= item[:answer_number] %></p>
      <p>Emission value: <%= item[:emission_value] %></p>
    </li>
    <% end %>
  </ul>
</div>


